<mapper namespace="postodo.aacii.mapper.freeboardMapper">

	<select id="selectFreeboardList"
			parameterType="postodo.aacii.net.board.dto.FreeboardDto"
			resultType="postodo.aacii.net.board.dto.FreeboardDto">
		SELECT	FREEBOARD_NO
				, FREEBOARD_TITLE
				, FREEBOARD_CONTENT
				, CREATE_IP
				, CREATE_ID
				, CREATE_TIME
				, UPDATE_IP
				, UPDATE_ID
				, UPDATE_TIME
				, DEL_YN
		FROM	POSTODO.BOARD_FREEBOARD
		WHERE
	</select>
	
	<insert id="insertFreeboard"
			parameterType="postodo.aacii.net.board.dto.FreeboardDto">
		INSERT INTO POSTODO.BOARD_FREEBOARD
		(
			FREEBOARD_TITLE
			, FREEBOARD_CONTENT
			, CREATE_IP
			, CREATE_ID
			, CREATE_TIME
			, DEL_YN
		)
		VALUES
		(
			#{freeboardTitle}
			, #{freeboardContent}
			, #{createIp}
			, #{createId}
			, TO_CHAR(SYSDATE, 'YYYYMMDDHH24MISS')
			, 'N'
		)
	</insert>

</mapper>